{
  "name": "signals.workshops.utkusarioglu.com",
  "dockerComposeFile": [
    "../.docker/docker-compose.dev.yml",
    "docker-compose.yml"
  ],
  "service": "signals-workshop",
  "workspaceFolder": "/utkusarioglu-com/workshops/signals",
  "customizations": {
    "vscode": {
      "settings": {
        "workbench.colorCustomizations": {
          "activityBar.background": "#430157",
          "activityBar.foreground": "#ffffff"
        }
      },
      "extensions": [
        "ms-python.python",
        "ms-toolsai.jupyter",
        "ms-python.black-formatter",
        "jatinchowdhury18.vscode-supercollider",
        "ms-vscode.powershell"
      ]
    }
  },
  "postCreateCommand": "scripts/post-create-command.sh"
}
